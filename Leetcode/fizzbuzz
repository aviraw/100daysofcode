class Solution {
    public List<String> fizzBuzz(int n) {
        List<String> l=new ArrayList<String>(); 
        for(int i=1;i<=n;i++)
        {
            String s="";
            if(i%3==0)
                s=s+"Fizz";
            if(i%5==0)
                s=s+"Buzz";
            if(i%3!=0&&i%5!=0)
                s=s+i;
            l.add(s);
        }
        return l;
    }
}
